{
  "name": "flutter-doc",
  "displayName": "flutter doc",
  "description": "show flutter widget doc",
  "version": "0.1.0",
  "publisher": "xiechuanlong",
  "engines": {
    "vscode": "^1.23.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:flutterDoc.show"
  ],
